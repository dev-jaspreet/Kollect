<% include ./partials/header %>
<% var count = 0; %>
<div class="container">
    <div class="row">
        <div class="col s12 m6 l3"></div>
        <div class="col s12 m6 l6 z-depth-1 mystyleother customborder">
            <h3>Feedback</h3>
            <hr>
            <form action="/optioninput/<%=foundset._id%>/checkdone" method="POST">
                <div class="input-field">
                    <h6>Created By: <%=foundset.creator.name%></h6>
                    <h6>Created On: <%=foundset.created.toDateString()%></h6>
                    <h6>Question Set: <%= foundset.name.toUpperCase()%></h6>
                </div>
                <hr>
                <%    for(var i = 0; i < foundset.key.length; i++){ %>
                <% if(foundset.key[i] == "checkbox"){%>
                <!--<div class="chips chips-placeholder"></div>-->
                <h6><%=foundset.question[i]%></h6>
                <div class="myDiv">
                    <div class="chips">
                        <input placeholder="huh" class="custom-class" name="check"><span><a onclick="optionfinal(<%=count%>)" 
                        class="atag right waves-effect waves-light btn-floating mystylebutton green darken-4">
                            <i class="material-icons">check</i></a></span>
                    </div>
                </div>
                <% count++; %>
                <% } %>
                <% } %>
                <div class="row">
                    <div class="hide-on-small-only show-on-medium-and-up">
                        <div class="col s12"><button type="submit" name="action"
                                class="col s12 waves-effect waves-light btn mystylebutton green darken-4"><i
                                    class="material-icons">check</i></button></div>
                    </div>
                    <div class="show-on-small hide-on-med-and-up">
                        <div class="col s12"><button type="submit" name="action"
                                class="col s12 waves-effect waves-light btn-large mystylebutton green darken-4"
                                style="border-radius:100px"><i class="material-icons">check</i></button></div>
                    </div>
                </div>
            </form>
        </div>
        <div class="col s12 m6 l3"></div>
    </div>
</div>
<script src="/scripts/option.js"></script>
<% include ./partials/footer %>