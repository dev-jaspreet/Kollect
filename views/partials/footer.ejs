<script>
    var toast = "<%=toast%>";
    M.AutoInit();
    $("select[required]").css({
        display: "inline",
        height: 0,
        padding: 0,
        width: 0
    });
    $(document).ready(function () {
        $('ul.tabs').tabs({
            swipeable: true,
            responsiveThreshold: 3000

        });
        $('input#registrationno').characterCounter();
    });
    if (toast && toast.length > 0) {
        M.toast({
            html: "<%=toast%>",
            classes: 'customtoast'
        })
    }
