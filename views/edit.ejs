<% include ./partials/header %>

<div class="container">
    <div class="row">
        <div class="col s12 m6 l3"></div>
        <div class="col s12 m6 z-depth-1 mystyleother customborder">
            <h3>Edit </h3>
            <form action="/display/<%=foundset._id%>?_method=PUT" method="POST">
                <div class="input-field">
                    <label for="name">Set Question Bank Name</label>
                    <input type="text" name="Question[name]" class="validate" id="name" value="<%=foundset.name%>"
                        required>
                    <small id="emailHelp" class="form-text text-muted">Needed</small>
                </div>
                <%    for(var i = 0; i < foundset.question.length; i++){ %>
                <div class="input-field">
                    <label for="question"><%= i+1 %>: Question Type: <%=foundset.key[i].toUpperCase()%> Needed</label>
                    <input type="text" name="Question[question]" class="validate" id="question"
                        value="<%= foundset.question[i] %>" required>
                </div>
                <%    } %>
                <button type="submit" class="waves-effect waves-light btn mystylebutton">Submit Changes</button>
                <a href="/display/<%=foundset._id%>" class="waves-effect waves-light btn mystylebutton">Cancel</a>
            </form>
        </div>
        <div class="col s12 m6 l3"></div>
    </div>
</div>

<% include ./partials/footer %>