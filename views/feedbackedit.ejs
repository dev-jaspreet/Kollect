<% include ./partials/header %>

<div class="container">
  <div class="row">
    <div class="col s12 m6 l3"></div>
    <div class="col s12 m6 l6 z-depth-1 mystyleother">
      <h3>Edit </h3>
      <hr>
      <form action="/feedbackedit/<%=foundans[0].id%>?_method=PUT" method="POST">
        <div class="input-field">
          <h6>Created By: <%=foundqn.creator.name%></h6>
          <h6>Created On: <%=foundqn.created.toDateString()%></h6>
          <h6>Question Set: <%= foundqn.name.toUpperCase()%></h6>
        </div>
        <hr>
        <%    for(var i = 0; i < foundqn.question.length; i++){ %>
        <div class="input-field">
          <label for="question"><%= i+1 %>: <%= foundqn.question[i] %></label>
          <input type="text" name="Answer[answer]" class="validate" id="question" value="<%=foundans[0].answer[i]%>">
        </div>
        <%    } %>
        <button type="submit" class="btn btn-primary">Submit Changes</button>
      </form>
    </div>
    <div class="col s12 m6 l3"></div>
  </div>
</div>

<% include ./partials/footer %>